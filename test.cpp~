#include <stdio.h>
#include <string.h>
#include <algorithm>
#include <vector>
#include <ctype.h>
#include <stack>
#include <iostream>
#define sigma_size 26
using namespace std;
const int Max =4000*1000+10;
int son[Max];
int bro[Max];
char ch[Max];
int val[Max];
int flag[Max];
int cnt;
long long ans;
void insert(const char *s)
{
    ans+=val[0];
    val[0]++;
    int i,n=strlen(s);
    int u=0;
    int v;
    for(i=0;i<n;i++)
    {
        bool found=false;
        for(v=son[u];v!=0;v=bro[v])
            if(ch[v]==s[i])
             {
                 found=true;
                 break;
             }
        if(!found)
        {
            v=cnt++;
            val[v]=0;
            flag[v]=0;
            ch[v]=s[i];
            bro[v]=son[u];
            son[u]=v;
            son[v]=0;
        }
        u=v;
        ans+=val[u]*2;
        val[u]++;
    }
    if(flag[v])ans+=flag[v];//这个是用于统计相同字符串的个数 开始没有这个一直WA 比如3 个a 那么要比较 6次
    flag[v]++;
    //ans=temp;
}
char str[1010];

int main()
{
   //  freopen("in.txt","r",stdin);
    int n,num=1;
    while(scanf("%d",&n),n)
    {
        ans=0;
        cnt=1;
        val[0]=0;
        son[0]=bro[0]=0;
        while(n--)
        {
            scanf("%s",str);
            insert(str);
        }
        printf("Case %d: %lld\n",num++,ans);
    }
    return 0;
}
